alias rspecf="rspec --tag ~slow:true"
alias php-server="php -S localhost:8000"

alias postgres-rm-pid="rm /usr/local/var/postgresql@9.5/postmaster.pid"
alias puma-dev-start="launchctl load ~/Library/LaunchAgents/io.puma.dev.plist"
alias puma-dev-stop="launchctl unload ~/Library/LaunchAgents/io.puma.dev.plist"
alias puma-dev-run="puma-dev -http-port 80 -https-port 443"
